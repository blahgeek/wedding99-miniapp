
.board {
    margin: 20rpx auto 20rpx;
}

.row {
    display: flex;
    justify-content: center;
}

.cell {
    display: inline-block;
    width: 128rpx;
    height: 128rpx;
    text-align: center;
    line-height: 128rpx;
    vertical-align: middle;
    border: 1px solid black;
}

.cell.active {
    background-color: #c0c0c0;
}

.bingo {
    margin: 20rpx auto 20rpx;
    text-align: center;
    font-size: 64rpx;
}
